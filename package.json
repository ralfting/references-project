{
  "name": "references-project",
  "version": "1.0.0",
  "description": "A tool to control your order of study about anything",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --require ./tests/setup.js",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --require ./tests/setup.js --watch",
    "test:coverage": "nyc npm run test",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=webpack.config.js",
    "start": "./node_modules/.bin/webpack-dev-server --open"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 80,
    "lines": 80,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/specs/**",
      "tests/setup.js"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "sourceMap": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ralph-learning/references-project.git"
  },
  "keywords": [
    "Javascript"
  ],
  "author": "Ralph Effting <ralfting@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ralph-learning/references-project/issues"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "postcss-loader": "^2.0.9",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "tailwindcss": "^0.3.0"
  }
}
